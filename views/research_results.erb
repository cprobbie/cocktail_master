<h2>search results displayed for keyword: <span><%= @movie_keyword %></span></h2>


<div class="result" action="/show">  
  <% result_hash.each do |drink| %>
  <a href="/show?i=<%= drink["idDrink"] %>">  
    <img src="<%= drink["Poster"] %>" alt=""><br>
    <%= drink["strDrink"] %>
  </a>
  <% end %>
</div>